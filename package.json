{
  "name": "node-gameboy",
  "version": "0.1.2",
  "description": "A Gameboy emulator written in JavaScript",
  "main": "lib/gameboy.js",
  "scripts": {
    "build": "browserify example/web.js -o static/bundle.js",
    "build-min": "browserify example/web.js | uglifyjs -cm > static/bundle.min.js",
    "watch": "watchify example/web.js -o static/bundle.js"
  },
  "keywords": [
    "gameboy",
    "nintendo",
    "emulator",
    "console",
    "handheld"
  ],
  "author": "Diego Acosta <diegonake@gmail.com>",
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browser": {
    "canvas": false,
    "debug": "./lib/debug.js"
  },
  "dependencies": {
    "canvas": "^1.4.0",
    "debug": "^2.2.0",
    "raf": "^3.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "gifencoder": "^1.0.6",
    "socket.io": "^1.5.1",
    "uglify-js": "^2.7.4",
    "watchify": "^3.7.0"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmacosta/node-gameboy.git"
  },
  "bugs": {
    "url": "https://github.com/dmacosta/node-gameboy/issues"
  },
  "homepage": "https://github.com/dmacosta/node-gameboy#readme",
  "engines": {
    "node": ">=6"
  }
}
