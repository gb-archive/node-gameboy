{
  "name": "node-gameboy",
  "version": "0.1.6",
  "description": "A Gameboy emulator written in JavaScript",
  "main": "lib/gameboy.js",
  "scripts": {
    "build": "browserify examples/web.js -o dist/bundle.js",
    "build-min": "browserify examples/web.js | uglifyjs -cm > dist/bundle.min.js",
    "build-all": "npm run build && npm run build-min",
    "watch": "watchify examples/web.js -o dist/bundle.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "gameboy",
    "nintendo",
    "emulator",
    "game",
    "console"
  ],
  "author": "Diego Acosta <diegonake@gmail.com>",
  "license": "MIT",
  "browserify": {
    "transform": [
      "brfs",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browser": {
    "canvas": false,
    "debug": "./lib/stubs/debug.js"
  },
  "dependencies": {
    "canvas": "^1.4.0",
    "debug": "^2.2.0",
    "raf": "^3.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "gifencoder": "^1.0.6",
    "socket.io": "^1.5.1",
    "uglify-js": "^2.7.4",
    "watchify": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nakardo/node-gameboy.git"
  },
  "bugs": {
    "url": "https://github.com/nakardo/node-gameboy/issues"
  },
  "homepage": "https://github.com/nakardo/node-gameboy#readme",
  "engines": {
    "node": ">=6"
  },
  "directories": {
    "example": "examples"
  }
}
