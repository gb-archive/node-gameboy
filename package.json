{
  "name": "node-gameboy",
  "version": "0.0.0",
  "description": "A gameboy emulator written in JavaScript",
  "main": "index.js",
  "scripts": {
    "build": "browserify example/web.js -o static/bundle.js",
    "watch": "watchify example/web.js -o static/bundle.js"
  },
  "keywords": [
    "gameboy",
    "nintendo",
    "emulator",
    "console",
    "handheld"
  ],
  "author": "Diego Acosta <diegonake@gmail.com>",
  "license": "MIT",
  "directories": {
    "test": "test"
  },
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "browser": {
    "canvas": false,
    "debug": "./lib/debug.js"
  },
  "dependencies": {
    "canvas": "^1.4.0",
    "debug": "^2.2.0",
    "raf": "^3.3.0"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "gifencoder": "^1.0.6",
    "watchify": "^3.7.0"
  }
}
